<GameThermalEngineTypeList>
    <version value="20241210"/>
    <!--MLBB-->
    <TYPE id="0">
        <!--画质设置：AIVRS/GPU-->
        <setting type_policy="policy_graphic" value="[0 60],[0 0 0 1001 1001]"/>
        <AMBIENT value="100">
            <!--壳温阈值-->
            <setting ambient_policy="shell_temp_threshold" value="45"/>
            <!--屏幕亮度调整范围-->
            <setting ambient_policy="screen_brightness_range" value="180,255"/>
            <!--音量调整范围-->
            <setting ambient_policy="volume_range" value="11,15"/>
            <!--壳温计算区间（速率），单位秒-->
            <setting ambient_policy="shell_window_size" value="180"/>
            <!--板温计算区间（平均值），单位秒-->
            <setting ambient_policy="pcb_window_size" value="30"/>
            <!--通知间隔，单位秒-->
            <setting ambient_policy="call_back_period" value="30"/>
            <!--壳温采样区间，单位秒-->
            <setting ambient_policy="shell_temp_sampling_period" value="10"/>
            <!--板温采样区间，单位秒-->
            <setting ambient_policy="pcb_temp_sampling_period" value="10"/>
            <!--多少秒平均帧率，单位秒-->
            <setting ambient_policy="fps_avg_period" value="1"/>
            <!--目标帧率计算次数-->
            <setting ambient_policy="target_fps_count" value="30"/>
            <!--默认帧率（不单独设目标帧率就走common）-->
            <FPS value="Common">
                <!--壳温预测方式的恢复温度间隔（距离壳温阈值n度恢复）-->
                <setting fps_policy="recover_shell_temp_interval" value="10"/>
                <!--主板温度方式的恢复温度间隔（距离每档温度n度恢复）-->
                <setting fps_policy="recover_pcb_temp_interval" value="1"/>
                <!--壳温预测方式的策略触发档位（当前壳温距离壳温阈值的剩余时间，单位分钟）-->
                <setting fps_policy="shell_time_interval" value="15,10,5"/>
                <!--主板温度方式的策略触发档位（当前板温距离板温阈值的剩余温度，单位度）-->
                <setting fps_policy="pcb_temp_interval" value="3,1,0"/>
                <!--壳温最低触发温度-->
                <setting fps_policy="min_shell_temp" value="35"/>
                <!--板温最低触发温度-->
                <setting fps_policy="min_pcb_temp" value="40"/>
                <!--限频策略底线-->
                <setting fps_policy="min_cpu_freq" value="0.5,0.5,0.5"/>
                <!--锁帧策略底线-->
                <setting fps_policy="min_fps" value="30"/>
                <!--默认场景-->
                <SCENE value="default">
                    <!--场景是否跟随温度（1：优先级：温度>场景/0：优先级：场景>温度）-->
                    <setting scene_policy="follow_temp_enable" value="1"/>
                    <!--屏幕亮度降低档位-->
                    <setting scene_policy="screen_brightness" value="unknown"/>
                    <!--音量降低档位-->
                    <setting scene_policy="volume" value="unknown"/>
                    <!--脚步声增强开关-->
                    <setting scene_policy="increased_footsteps" value="0"/>
                    <!--频率天花板被限制的档位-->
                    <setting scene_policy="cpu_freq" value="unknown"/>
                    <!--锁帧的档位-->
                    <setting scene_policy="fps" value="0.9,0.85,0.85"/>
                </SCENE>
            </FPS>
            <FPS value="90">
                <setting fps_policy="recover_shell_temp_interval" value="10"/>
                <setting fps_policy="recover_pcb_temp_interval" value="1"/>
                <setting fps_policy="shell_time_interval" value="15,10,5"/>
                <setting fps_policy="min_shell_temp" value="35"/>
                <setting fps_policy="min_pcb_temp" value="40"/>
                <setting fps_policy="pcb_temp_interval" value="30,0,0"/>
                <setting fps_policy="min_cpu_freq" value="0.5,0.5,0.5"/>
                <setting fps_policy="min_fps" value="70"/>
                <SCENE value="default">
                    <setting scene_policy="follow_temp_enable" value="1"/>
                    <setting scene_policy="screen_brightness" value="unknown"/>
                    <setting scene_policy="volume" value="unknown"/>
                    <setting scene_policy="increased_footsteps" value="0"/>
                    <setting scene_policy="cpu_freq" value="unknown"/>
                    <setting scene_policy="fps" value="80,80,80"/>
                </SCENE>
            </FPS>
        </AMBIENT>
    </TYPE>
